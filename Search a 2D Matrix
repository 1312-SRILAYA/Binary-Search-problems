class Solution:
    def searchMatrix(self, matrix: list[list[int]], target: int) -> bool:
        n=len(matrix) # number of rows
        m=len(matrix[0]) # number of cols
        row=0
        col=m-1
        while(row<n and col>=0):
            if(matrix[row][col]==target):
                return True
            elif(target>matrix[row][col]):
                row+=1
            elif(target<matrix[row][col]):
                col-=1
        return False